<!DOCTYPE mycat:crypto SYSTEM "crypto.dtd">
<mycat:crypto xmlns:mycat="http://org.opencloudb/">
	<crypto name="c001">
		<column name="name" algorithm="symmetric-cryto" />
	</crypto>
	<function name="symmetric-cryto" class="org.opencloudb.crypto.function.SymmetricCryto">
		<property name="size">128</property><!-- 默认是128 -->
		<property name="type">AES</property><!-- 对称加密算法 -->
	</function>
</mycat:crypto>
